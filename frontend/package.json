{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "copy:backend": "copyfiles -u 1 dist/**/* ../backend/dist/public",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "core-js": "^3.6.5",
    "debounce": "^1.2.1",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "scrypt-async": "^2.0.1",
    "scrypt-js": "^3.0.1",
    "sha.js": "^2.4.11",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^4.12.0",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^8.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unreachable": "off"
